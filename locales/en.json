{
  "cli": {
    "name": "marhup",
    "description": "Generate PowerPoint (PPTX) from Markdown with grid-based layout",
    "version": "0.1.7",
    "options": {
      "output": "Output file name (single file only)",
      "outputDir": "Output directory (for multiple files)",
      "theme": "Theme name",
      "grid": "Default grid size",
      "watch": "Watch mode",
      "config": "Path to configuration file",
      "inputFiles": "Input Markdown files (multiple allowed)"
    },
    "examples": {
      "basic": "$ marhup input.md -o presentation.pptx",
      "theme": "$ marhup input.md --theme corporate --grid 16x10",
      "watch": "$ marhup input.md --watch",
      "batch": "$ marhup file1.md file2.md file3.md -d ./presentations"
    },
    "commands": {
      "generate": {
        "description": "Generate PPTX from Markdown",
        "examples": {
          "basic": "$ marhup generate input.md -o presentation.pptx",
          "theme": "$ marhup generate input.md --theme corporate --grid 16x10",
          "batch": "$ marhup generate file1.md file2.md file3.md -d ./presentations"
        }
      },
      "undo": {
        "description": "Undo the last generation",
        "examples": {
          "basic": "$ marhup undo output.pptx"
        },
        "note": "Restores from the backup file created during the last generation."
      },
      "export": {
        "description": "Parse Markdown and export as JSON",
        "examples": {
          "basic": "$ marhup export input.md -o document.json"
        }
      },
      "import": {
        "description": "Generate PPTX from JSON",
        "examples": {
          "basic": "$ marhup import document.json -o presentation.pptx"
        }
      },
      "preview": {
        "description": "Preview Markdown layout",
        "examples": {
          "basic": "$ marhup preview input.md",
          "grid": "$ marhup preview input.md --grid 16x10"
        }
      },
      "watch": {
        "description": "Watch file and auto-regenerate",
        "examples": {
          "basic": "$ marhup watch input.md -o presentation.pptx",
          "theme": "$ marhup watch input.md --theme corporate"
        },
        "note": "In watch mode, files are automatically regenerated when the input file changes. Press Ctrl+C to exit."
      },
      "help": {
        "description": "Show detailed usage and syntax guide"
      }
    },
